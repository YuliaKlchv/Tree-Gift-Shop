Cart([]);\r\n    getTotal();\r\n  };\r\n\r\n  let sum = function (items, prop) {\r\n    return items.reduce(function (a, b) {\r\n      return a + b[prop];\r\n    }, 0);\r\n  };\r\n\r\n  function showMessage(arr) {\r\n    return arr.length === 0 ? (\r\n      <>\r\n        No items here yet. Wanna go shopping? <Link to=\"/\">See trees</Link>\r\n      </>\r\n    ) : (\r\n      <p>You have {arr.length} items in your cart.</p>\r\n    );\r\n  }\r\n\r\n  const getTotal = () => {\r\n    const total = [...cart];\r\n    const res = total.reduce((prev, item) => {\r\n      return prev + item.price * item.count;\r\n    }, 0);\r\n    setTotal(res);\r\n  };\r\n\r\n  return (\r\n    <CartContext.Provider\r\n      value={{\r\n        cart,\r\n        reduceCount,\r\n        increaseCount,\r\n        total,\r\n        setTotal,\r\n        getTotal,\r\n        setCart,\r\n        addToCart,\r\n        removeFromCart,\r\n        sum,\r\n        showMessage,\r\n        resetCart,\r\n      }}\r\n    >\r\n      {props.children}\r\n    </CartContext.Provider>\r\n  );\r\n};\r\n","C:\\GITHUB\\Tree-Gift-Shop\\src\\ProductContext.js",[],"C:\\GITHUB\\Tree-Gift-Shop\\src\\components\\CartExtraPage\\Cart.js",["141","142","143","144","145"],"import React from \"react\";\r\nimport \"./Cart.css\";\r\nimport { CartContext } from \"../../CartContext\";\r\nimport { useContext, useEffect } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nfunction Cart(props) {\r\n  const {\r\n    cart,\r\n    addToCart,\r\n    reduceCount,\r\n    getTotal,\r\n    total,\r\n    setTotal,\r\n    itemsCount,\r\n    increaseCount,\r\n    removeFromCart,\r\n    sum,\r\n    showMessage,\r\n    resetCart,\r\n  } = useContext(CartContext);\r\n\r\nuseEffect(() => {\r\n  getTotal()\r\n}, [])\r\n\r\n  return (\r\n    <div className=\"App\">\r\n      <div className=\"cart-container\">\r\n        <div className=\"cart-banner\">\r\n          <Link to=\"/\">\r\n            <h2 className=\"cart-close\">X</h2>\r\n   