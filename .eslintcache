id === id) {\r\n        item.count += 1;\r\n      }\r\n    });\r\n    setCart(cartUpdater);\r\n    getTotal();\r\n  };\r\n\r\n  const resetCart = () => {\r\n    setCart([]);\r\n    getTotal();\r\n  };\r\n\r\n  let sum = function (items, prop) {\r\n    return items.reduce(function (a, b) {\r\n      return a + b[prop];\r\n    }, 0);\r\n  };\r\n\r\n  function showMessage(arr) {\r\n    return arr.length === 0 ? (\r\n      <>\r\n        No items here yet. Wanna go shopping? <Link to=\"/\">See trees</Link>\r\n      </>\r\n    ) : (\r\n      <p>You have {arr.length} items in your cart.</p>\r\n    );\r\n  }\r\n\r\n  const getTotal = () => {\r\n    const total = [...cart];\r\n    const res = total.reduce((prev, item) => {\r\n      return prev + item.price * item.count;\r\n    }, 0);\r\n    setTotal(res);\r\n  };\r\n\r\n  return (\r\n    <CartContext.Provider\r\n      value={{\r\n        cart,\r\n        reduceCount,\r\n        increaseCount,\r\n        total,\r\n        setTotal,\r\n        getTotal,\r\n        setCart,\r\n        addToCart,\r\n        removeFromCart,\r\n        sum,\r\n        showMessage,\r\n        resetCart,\r\n      }}\r\n    >\r\n      {props.children}\r\n    </CartContext.Provider>\r\n  );\r\n};\r\n","C:\\Users\\a-713\\OneDrive\\Desktop\\TreeGiftShop\\ReactApp\\tree-gift-shop\\src\\components\\FooTer\\Footer.js",[],"C:\\Users\\a-713\\OneDrive\\Desktop\\TreeGiftShop\\ReactApp\\tree-gift-shop\\src\\components\\NavigationBar\\Navigation.js",["140"],"C:\\Users\\a-713\\OneDrive\\Desktop\\TreeGiftShop\\ReactApp\\tree-gift-shop\\src\\components\\LoginSection\\LoginSection.js",["141","142","143","144","145"],"import React, { useState, useEffect } from 'react'\r\nimport FormLogin from '../NavigationBar/FormLogin'\r\nimport { BrowserRouter, Route, Switch } from 'react-router-dom';\r\nimport useToken from '../../useToken'\r\n\r\nexport default function LoginSection() {\r\n  const { token, setToken } = useToken();\r\n  \r\n\r\n    if(!token) {\r\n    return (\r\n