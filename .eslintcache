ut = () => {\r\n    setToken([])\r\n  }\r\n  \r\n    const notLoggedPopover = (\r\n        <Popover id=\"popover-basic\">\r\n          <Popover.Title as=\"h3\">Login or Sign up</Popover.Title>\r\n          <Popover.Content>\r\n            <AccordionLog/>\r\n          </Popover.Content>\r\n        </Popover>\r\n      );\r\n\r\n      const loggedInPopover = (\r\n        <Popover id=\"popover-basic\">\r\n          <Popover.Title as=\"h3\">Already logged in</Popover.Title>\r\n          <Popover.Content>\r\n            <button onClick={logout}>Logout</button>\r\n          </Popover.Content>\r\n        </Popover>\r\n      );\r\n      \r\n      const NotLogged = () => (\r\n        <OverlayTrigger trigger=\"click\" placement=\"bottom\" overlay={notLoggedPopover}>\r\n          <Button variant=\"light\" className=\"icons\">\r\n              <PersonCircle size={25} />\r\n            </Button>\r\n        </OverlayTrigger>\r\n      );\r\n\r\n      const Logged = () => (\r\n        <OverlayTrigger trigger=\"click\" placement=\"bottom\" overlay={loggedInPopover}>\r\n          <Button variant=\"light\" className=\"icons\">\r\n              <PersonCircle size={25} />\r\n            </Button>\r\n        </OverlayTrigger>\r\n      );\r\n\r\n\r\n      const Check = () => {\r\n        if(!token) {\r\n          return <NotLogged />\r\n        } else {\r\n          return <Logged />\r\n        }\r\n      }\r\n\r\n\r\n    return(\r\n        <div>\r\n            <Check />\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default OverlayNav;\r\n\r\n","C:\\Users\\a-713\\OneDrive\\Desktop\\TreeGiftShop\\ReactApp\\tree-gift-shop\\src\\components\\NavigationBar\\FormLogin.js",["163"],"import Form from 'react-bootstrap/Form';\r\nimport Button from 'react-bootstrap/Button';\r\nimport React, { useState } from \"react\";\r\nimport PropTypes from \"prop-types\";\r\n\r\n\r\nasync function loginUser(credentials) {\r\n  return fetch(\"http://localhost:9000/login\", {\r\n    method: \"POST\",\r\n    header